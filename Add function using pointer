

#include <stdio.h>
int add(int *ptr1, int *ptr2){
    int c = *ptr1 + *ptr2;
}
int main()
{
     int a , b;
    printf("enter the value in a : ");
    scanf("%d",&a);
    printf("enter the value in b : ");
    scanf("%d",&b);
    
    int re =  add(&a,&b);
    printf("Addition is : %d", re);

    return 0;
}
