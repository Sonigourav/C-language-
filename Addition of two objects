#include <iostream>
class A{
    private:
        int x;
    public : 
        A(int a){
           x = a; 
        }
    void operator +(A);
    // void display();
   
};

void A :: operator +(A a){
    int m = x + a.x;
    std::cout << a.x << std::endl;
    std::cout << "the re is : "<<m << std::endl;
}
int main()
{
    A a1(5);
    
    A a2(6);
    A a3(9);
    a1+a2;
    

    return 0;
}
