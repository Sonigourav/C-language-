#include <stdio.h>
#include <string.h>
struct Student{
    char name[50];
    int age;
    int marks;
};
int main()
{
    struct Student s[3];
    
    strcpy(s[1].name, "gourav");
    s[1].age = 23;
    s[1].marks = 89;
    
    strcpy(s[2].name,"Anubhav");
    s[2].age = 23;
    s[2].marks = 81;
    
    strcpy(s[3].name, "Anurag");
    s[3].age = 23;
    s[3].marks = 90;
    
    for(int i=1;i<=3;i++){
        printf("Name: %s\n", s[i].name);
        printf("Age: %d\n", s[i].age);
        printf("Marks: %d\n", s[i].marks);
        printf("\n");
    }

    return 0;
}
