#include<iostream>
using namespace std;

class A{
    public: 
        int add(int a, int b){
            return a + b ;
        }
        
        int add(int a, int b, int c){
            return a + b + c;
        }
};
int main(){
    A a1;
   std::cout <<  a1.add(3,5) << std::endl;
   std::cout <<  a1.add(3,4,5) << std::endl;
}
