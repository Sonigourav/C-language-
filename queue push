#include<iostream>
#include<queue>
using namespace std;

int main(){
    std::queue<int>myqueue;
    
    myqueue.push(0);
    myqueue.push(1);
    myqueue.push(2);
    myqueue.push(3);
    
    while(!myqueue.empty()){
        std::cout << ' '<< myqueue.front() << std::endl;
        myqueue.pop();
    }
    
}
