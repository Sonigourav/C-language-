#include<bits/stdc++.h>
using namespace std;

void showStack(std::stack<int> s){
    while(!s.empty()){
        std::cout << ' '<< s.top() << std::endl;
        s.pop();
    }
}

int main(){
    std::stack<int>s ;
    
    s.push(0);
    s.push(1);
    s.push(2);
    s.push(3);
    s.push(4);
    
    cout<<"The stack is "<< std::endl;;
    showStack(s);
    
    
    std::cout << "pop" << std::endl;
    s.pop();
    showStack(s);
    
    return 0;
}
