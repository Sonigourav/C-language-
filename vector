#include<iostream>
#include<vector>
using namespace std;

int main(){
    
    vector<int> vec;
    int i ;
    
    std::cout << "vector size" << vec.size() << std::endl;
    
    for(i=0;i<5;i++){
        vec.push_back(i);
    }
    
    std::cout << "Extented vector size  : "<<vec.size() << std::endl;
    
    for(i=0;i<5;i++){
        std::cout << vec[i] << std::endl;
    }
    
    vector<int>:: iterator v = vec.begin() ;
        while(v != vec.end()){
            std::cout << "Value of v is : " << *v << std::endl;
            v++;
            
        }
        return 0;
}



      
